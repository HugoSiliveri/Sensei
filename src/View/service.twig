                <div class="coteDroit service overflow">
                    <div class="miseEnPage">
                        <button class="boutonDisparition">-</button>
                        <div class="miseEnPage2 maxWidth">
                            <h2>Département</h2>
                            <div class="disparition">
                                <div class="miseEnPage">
                                    <button class="boutonDisparition">-</button>
                                    <div class="miseEnPage2">
                                        <h2>Semestre indéfini</h2>
                                        <div class="disparition">
                                            {% if indefinisDepartement|length > 0 %}
                                                <div class="disparition">
                                                    {% for i in 0..indefinisDepartement|length-1 %}
                                                        <div class="miseEnPage2">
                                                            <div class="infosServiceConteneur">
                                                                {% set nbGroupesCM = indefinisDepartement[i][0].getNbGroupesCM %}
                                                                {% set nbGroupesTD = indefinisDepartement[i][0].getNbGroupesTD %}
                                                                {% set nbGroupesTP = indefinisDepartement[i][0].getNbGroupesTP %}
                                                                {% set nbGroupesStage = indefinisDepartement[i][0].getNbGroupesStage %}
                                                                {% set nbGroupesTerrain = indefinisDepartement[i][0].getNbGroupesTerrain %}
                                                                {% set nbGroupesInnovation = indefinisDepartement[i][0].getNbGroupesInnovationPedagogique %}
                                                                {% set somme = nbGroupesCM + nbGroupesTD + nbGroupesTP + nbGroupesStage + nbGroupesTerrain + nbGroupesInnovation %}
                                                                <button class="boutonDisparition">+</button>
                                                                <p class="infosService2">{{ indefinisDepartement[i][1].getIdUSReferentiel }}
                                                                    {{ indefinisDepartement[i][0].getLibUSA }} ({{ somme }} groupe(s) )
                                                                </p>
                                                            </div>
                                                            <div class="disparition2 affichageInfoService">
                                                                <div class="infosSupplementaires">
                                                                    {% if nbGroupesCM > 0 %}
                                                                        {% for k in 0..nbGroupesCM-1 %}
                                                                            <div class="miseEnPage">
                                                                                {% include "templates/slider.twig" %}
                                                                                <p>CM {{k+1}} ( {{ indefinisDepartement[i][0].getHeuresCM }} h )</p>
                                                                            </div>
                                                                        {% endfor %}
                                                                    {% endif %}
                                                                    {% if nbGroupesTD > 0 %}
                                                                        {% for k in 0..nbGroupesTD-1 %}
                                                                            <div class="miseEnPage">
                                                                                {% include "templates/slider.twig" %}
                                                                                <p>TD {{k+1}} ( {{ indefinisDepartement[i][0].getHeuresTD }} h )</p>
                                                                            </div>
                                                                        {% endfor %}
                                                                    {% endif %}
                                                                    {% if nbGroupesTP > 0 %}
                                                                        {% for k in 0..nbGroupesTP-1 %}
                                                                            <div class="miseEnPage">
                                                                                {% include "templates/slider.twig" %}
                                                                                <p>TP {{k+1}} ( {{ indefinisDepartement[i][0].getHeuresTP }} h )</p>
                                                                            </div>
                                                                        {% endfor %}
                                                                    {% endif %}
                                                                    {% if nbGroupesStage > 0 %}
                                                                        {% for k in 0..nbGroupesStage-1 %}
