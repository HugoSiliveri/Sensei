{% block indefinis_departement %}
    {% if voeuxNonVacataires|length > 1 %}
        {% for l in 0..voeuxNonVacataires|length-1 %}
            {% for voeu in voeuxNonVacataires[l] %}
                <div class="enLigne">
                    {% for voeuMemeId in voeu %}
                        {% if voeuMemeId.idUSReferentiel == indefinisDepartement[i][1].getIdUSReferentiel and
                            voeuMemeId.typeIntervention == typeIntervention and voeuMemeId.numGroupe == k+1 %}
                            <p class="infosService"> {{ voeuMemeId.nom }} {{ voeuMemeId.prenom }} {{ voeuMemeId.volumeHoraire }}</p>
                        {% endif %}
                    {% endfor %}
                </div>
            {% endfor %}
        {% endfor %}
    {% endif %}
